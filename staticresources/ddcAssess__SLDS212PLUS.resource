html {
    font-family: sans-serif;
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%; }

.ddcAssess input[type="button"] {
    -webkit-appearance: button;
    cursor: pointer; }

.ddcAssess input[disabled] {
    cursor: default; }

.ddcAssess:not(html), body {
    font-size: 0.8125rem;
    background: transparent; }

body {
    margin: 0; }
