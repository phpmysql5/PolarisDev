n to the next action in our action sequence\n               dfd.resolve(); \n            },\n            error: function(){\n                // Run on-error actions\n                dfd.reject();\n            }\n        });\n\n        return dfd.promise();\n\t});\n\t\n\t// Update a Component Pack's cache\n\tregisterSnippet('skuid.updateCache',function(){\n\t    var params = arguments[0],\n\t        row = params.item.row,\n          resourceUrl = skuid.utils.getStaticResourceUrl(\n            row.skuid__Resource_Name__c,\n            row.skuid__Resource_Namespace__c,\n            row.skuid__Runtime_Definition_File__c || 'skuid_runtime.json',\n            new Date().getTime()\n          ),\n          pageTitleEditor = skuid.component.getByType('pagetitle')[0].element.data('object').editor;\n        \n        $.get(resourceUrl, function( data ) {\n            \n            var packDef;\n            \n            try {\n                packDef = JSON.parse(data);\n            }\n            catch(err) {\n                pageTitleEditor.handleMessages([{\n                    message: 'Your package definition is not valid JSON.',\n                    severity: 'ERROR'\n                }]); \n            }\n            \n            if (packDef) {\n                var chunks = [];\n                var chunkSize = 255;\n                \n                data = JSON.stringify(packDef);\n                \n                while (data) {\n                    if (data.length &lt; chunkSize) {\n                        chunks.push(data);\n                        break;\n                    }\n                    else {\n                        chunks.push(data.substr(0, chunkSize));\n                        data = data.substr(chunkSize);\n                    }\n                }\n                \n                var updateObj = {};\n                \n                if (chunks.length &lt; 10) {\n                    \n                    for (i=0; i&lt;10; i++) {\n                        var chunkNum = (i&lt;10 ? '0' + (i+1) : (i+1));\n                        var chunkVal = chunks[i] || '';\n                        updateObj['skuid__Package_Cache' + chunkNum + '__c'] = chunkVal;\n                    }\n                    \n                    params.model.updateRow(row,updateObj);\n                    params.model.save();\n                    \n                } else {\n                    pageTitleEditor.handleMessages([{\n                        message: 'Your package definition is too large to be cached.',\n                        severity: 'ERROR'\n                    }]);     \n                }\n            }\n        });\n\t});\n\t\n\t// Copy per-Profile prefs over from the Org Default\n\tregisterSnippet('skuid.settings.copyPrefsFromOrgDefault',function(){\n\t    var params = arguments[0];\n        var row = params.row;\n        var model = params.model;\n        \n        var OrgDefaultsModel = skuid.$M('stng__OrgPreferences');\n        var orgDefaults = OrgDefaultsModel.getFirstRow();\n        var rowUpdates = {};\n        \n        $.each(OrgDefaultsModel.fields,function(){\n           if (skuid.utils.startsWith(this.id,'skuid__') &amp;&amp; (this.id !== 'skuid__Use_Component_Packs__c')) {\n               rowUpdates[this.id] = OrgDefaultsModel.getFieldValue(orgDefaults,this.id,true);\n           } \n        });\n        \n        model.updateRow(row,rowUpdates,{initiatorId:params.initiatorId});\n\t});\n\t\n\t// Create a new Skuid Theme StaticResource file\n\tregisterSnippet('skuid.theme.createThemeResource',function(){\n\t    var params = arguments[0],\n        \t$ = skuid.$,\n            dfd = $.Deferred(),\n            row = params.row,\n            model = params.model;\n            \n        row.skuid__Skuid_Version_Date__c = skuid.version.date;\n        \n        var resourceNamespace = row.skuid__Resource_Namespace__c;\n        var baseTheme = row.BaseTheme;\n        var themeName = row.Name;\n        var isMobile = row.skui